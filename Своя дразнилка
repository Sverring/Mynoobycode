var randomBodyParts = ["вагина", "очко", "яйца","ебало","мамка","пиструн", "сиськи", "живот", "глаза", "рот", 
"щечки","зубы", "попец", "пресс","руки","ноги", "пальцы", "ногти", "коленки", "улыбка","взгляд", "лоб", "нос","брови", 
"голос", "мозг", "интеллект", "хлеборезка", "губы", "кадык","плечо","спина","шутки", "семя", "волосы", "прическа", "брюки", 
"рубашка","ботинки", "куртка", "шапка", "кепка", "футболка", "костюм", "образование", "юмор"];
var randomAdjectives = ["вонючая", "унылая", "грязная", "ебантяйская", "еврейская", "черная", "бомжатская", 
"хачёвская", "пидорская", "охуевшая","фашисткая", "сутулая", "больная", "нищебродская", "отсталая","шлюхи", 
"залупы", "мамкина"];
var randomWords = ["ослиха", "куртизанка", "мышь", "давалка", "булка", "говяха на ляхе", "резиновая кукла", 
"боль", "страшилка", "суета", "дискриминация", "кастрация", "казнь", "кара", "старость", "совесть", "принципиальность", 
"корица", "жизнь", "поездка", "любовь", "смерть", "удача","губка для мытья посуды","ложка в салате","больница", "страна",
"анаша","радость для онаниста", "расплата"];
// Выбираем из первого массива:
var randomBodyPart = randomBodyParts [Math.floor (Math.random ()*randomBodyParts.length)];
//Выбираем слово из второго массива:
var randomAdjective = randomAdjectives [Math.floor (Math.random ()*randomAdjectives.length)];
//Выбираем слово из третьего массива:
var randomWord = randomWords [Math.floor(Math.random ()*randomWords.length)];
//Соединяем все в предложение:
var Randomresult = "У тебя "+ randomBodyPart+" словно "+randomAdjective+" "+randomWord+"!!!"; 
Randomresult;
