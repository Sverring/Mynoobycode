var randomBodyParts = ["вагина", "очко", "яйца"];
var randomAdjectives = ["вонючая", "унылая", "грязная"];
var randomWords = ["ослиха", "куртизанка", "мышь", "давалка", "булка"];
// Выбираем из первого массива:
var randomBodyPart = randomBodyParts [Math.floor (Math.random ()*3)];
//Выбираем слово из второго массива:
var randomAdjective = randomAdjectives [Math.floor (Math.random ()*3)];
//Выбираем слово из третьего массива:
var randomWord = randomWords [Math.floor(Math.random ()*5)];
//Соединяем все в предложение:
var Randomresult = "У тебя "+ randomBodyPart+" словно "+randomAdjective+" "+randomWord+"!!!"; 
Randomresult;
"У тебя очко словно грязная мышь!!!"

/* Второй раз ,но с использованием .length */
var randomBodyParts = ["вагина", "очко", "яйца"];
var randomAdjectives = ["вонючая", "унылая", "грязная"];
var randomWords = ["ослиха", "куртизанка", "мышь", "давалка", "булка"];
// Выбираем из первого массива:
var randomBodyPart = randomBodyParts [Math.floor (Math.random ()*randomBodyParts.length)];
//Выбираем слово из второго массива:
var randomAdjective = randomAdjectives [Math.floor (Math.random ()*randomAdjectives.length)];
//Выбираем слово из третьего массива:
var randomWord = randomWords [Math.floor(Math.random ()*randomWords.length)];
//Соединяем все в предложение:
var Randomresult = "У тебя "+ randomBodyPart+" словно "+randomAdjective+" "+randomWord+"!!!"; 
Randomresult;
"У тебя яйца словно вонючая давалка!!!"
// Использование array.length (который мы пишем, умножая на Math.random) позволяет не менять код, если длина массива изменится.

// Итоговую переменную можно задать через массив, пробелы между словами в котором можно сделать при помощи метода join (" ")
/* Лишь в одном случае пробел не нужен — между randomWord и «!!!». В этом случае мы используем оператор +, чтобы сое-
динить строки без пробела.*/
var Randomresult = ["У тебя", randomBodyPart, "словно", randomAdjective, randomWord+"!!!"].join (" "); 
Randomresult;

//Изощренная дразнилка
var randomBodyParts = ["вагина", "очко", "яйца"];
var randomAdjectives = ["вонючая", "унылая", "грязная"];
var randomAnimalBodyParts = ["ухо", "горло","член", "глаз", "нога"];
var randomAnimals = ["ослихи", "тигра", "мыши", "овцы", "козла"];
var randomBodyPart = randomBodyParts [Math.floor (Math.random ()*randomBodyParts.length)];
var randomAdjective = randomAdjectives [Math.floor (Math.random ()*randomAdjectives.length)];
var randomAnimalBodyPart = randomAnimalBodyParts [Math.floor (Math.random ()*randomAnimalBodyParts.length)];
var randomAnimal = randomAnimals [Math.floor (Math.random ()*randomAnimals.length)];
var Result = ["У тебя"+" "+randomBodyPart+" "+"еще более "+randomAdjective+" ,чем "+randomAnimalBodyPart+" у "+randomAnimal];
Result;

//Результат можно записать и через .join
var Result = ["У тебя",randomBodyPart,"еще более",randomAdjective+",чем",randomAnimalBodyPart,"у",randomAnimal+"."].join(" ");
Result;
"У тебя яйца еще более вонючая,чем глаз у овцы."

